
public class Main {
	public static void main(String[] args) {
		
	}
}

class Student {
	private String name;
	private double kor;
	private double eng;
	private int avg;
	
	Student(String name, double kor, double eng) {
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.avg = (int)(kor + eng) / 2;
	}
}
